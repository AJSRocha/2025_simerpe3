@article{roa-ureta_estimation_2021,
  title = {Estimation of the Spawning Stock and Recruitment Relationship of {{{\emph{Octopus}}}}{\emph{ Vulgaris}} in {{Asturias}} ({{Bay}} of {{Biscay}}) with Generalized Depletion Models: Implications for the Applicability of {{MSY}}},
  shorttitle = {Estimation of the Spawning Stock and Recruitment Relationship of {{{\emph{Octopus}}}}{\emph{ Vulgaris}} in {{Asturias}} ({{Bay}} of {{Biscay}}) with Generalized Depletion Models},
  author = {{Roa-Ureta}, Rub{\'e}n H and {Fern{\'a}ndez-Rueda}, M del Pino and Acu{\~n}a, Jos{\'e} Luis and Rivera, Antonella and {Gonz{\'a}lez-Gil}, Ricardo and {Garc{\'i}a-Fl{\'o}rez}, Luc{\'i}a},
