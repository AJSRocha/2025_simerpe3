@article{bouch_comparative_2021,
  title = {Comparative Performance of Data-Poor {{CMSY}} and Data-Moderate {{SPiCT}} Stock Assessment Methods When Applied to Data-Rich, Real-World Stocks},
  author = {Bouch, Paul and Minto, C{\'o}il{\'i}n and Reid, Dave G},
