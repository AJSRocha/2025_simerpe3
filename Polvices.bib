@article{soto_effects_2022,
  title = {Effects of Misreporting Landings, Discards, and {{Catch Per Unit}} of {{Effort}} Index in State-Space Production Models: The Case of Black Hake in Northwest {{Africa}}},
  shorttitle = {Effects of Misreporting Landings, Discards, and {{Catch Per Unit}} of {{Effort}} Index in State-Space Production Models},
  author = {Soto, Mar{\'i}a and {Fern{\'a}ndez-Peralta}, Lourdes and Pennino, Maria Grazia and Kokkalis, Alexandros and Rey, Javier and Salmer{\'o}n, Francisca and Li{\'e}bana, Mar{\'i}a and Meissa, Beyah and Kell, Laurie},
