  title = {Enhancing the Utility of Known-Biomass Production Models: A Case Study of the {{Bay}} of {{Biscay}} and {{Iberian Coast}} Ecoregion},
  shorttitle = {Enhancing the Utility of Known-Biomass Production Models},
  author = {Yosri Zanni, Mohamed and {Cousido-Rocha}, Marta and Cervi{\~n}o, Santiago and Pennino, Maria Grazia},
  year = {2024},
