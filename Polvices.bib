@article{wang_steroid_2022,
  title = {Steroid Hormones of the Octopus Self-Destruct System},
  author = {Wang, Z. Yan and Pergande, Melissa R. and Ragsdale, Clifton W. and Cologna, Stephanie M.},
  year = {2022},
